{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "babel"
  ],
  "rules": {
    "comma-dangle": 0,
    "prefer-const": 1,
    "spaced-comment": 0,
    "func-names": 0,
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true } }],
    "key-spacing": [2, { "align": "value" }],
    "space-in-parens": [2, "never"],
    "quotes": [2, "double"],
    "object-curly-spacing": [2, "never"]
  }
}
